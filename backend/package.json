{
  "name": "backend-main",
  "version": "1.0.0",
  "description": "Api Graphql",
  "author": "Daniel Suarez",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20",
    "yarn": ">=1",
    "npm": "please use yarn"
  },
  "os": [
    "darwin",
    "linux",
    "!win32"
  ],
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev2": "nest start --watch",
    "start:dev": "$PWD/scripts/start.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "$PWD/scripts/start-test.sh --watchAll --detectOpenHandles",
    "test:watch": "$PWD/scripts/start-test.sh --watch --detectOpenHandles",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test env-cmd -f $PWD/.env.development jest --config ./test/jest.e2e.json",
    "test:generate-queries": "gqlg --schemaFilePath $PWD/schema.gql --destDirPath $PWD/test/gql && graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@as-integrations/fastify": "^2.1.1",
    "@aws-sdk/client-s3": "^3.569.0",
    "@blazity/nest-file-fastify": "^1.0.0",
    "@faker-js/faker": "^8.4.1",
    "@fastify/multipart": "^8.2.0",
    "@mercuriusjs/gateway": "^3.0.0",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.4",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@songkeys/nestjs-redis": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-parse": "^5.5.5",
    "gql-generator": "^2.0.0",
    "graphql": "^16.8.1",
    "graphql-type-object-id-no-deps": "^1.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongodb": "4",
    "mongoose": "^8.2.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "mongoose-paginate-v2": "^1.8.0",
    "node-xlsx": "^0.24.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "sharp": "^0.33.4",
    "reflect-metadata": "^0.2.1",
    "res": "^0.4.0",
    "rxjs": "^7.8.1",
    "webpack": "^5.90.3",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
